{% layout 'password' %}
      
  <div class="utility-page-wrap">
    {% form 'storefront_password' %}<div class="utility-page-content-copy w-password-page w-form">
      {% unless form.posted_successfully? %}<div udesly-add-class="utility-page-form w-password-page"></div><img src="https://d3e54v103j8qbb.cloudfront.net/static/password-page-lock.832ca8e2c9.svg" alt="" class="image-60"><input type="password" autofocus="true" maxlength="256" name="password" placeholder="Enter your password" class="text-field-2 w-password-page w-input"><input type="submit" value="Submit" data-wait="Please wait..." class="sr-button w-password-page w-button">
        
        
        
      {% endunless %}
    </div><div class="utility-page-content-copy w-password-page w-form">{% if form.errors %}<div class="w-password-page w-form-fail" style="display: inherit;">
          <div>{% include 'form-errors' %}</div>
        </div>{% endif %}</div>{% endform %}
  </div>
  <script src="https://d3e54v103j8qbb.cloudfront.net/js/jquery-3.5.1.min.dc5e7f18c8.js?site=5f61d645b88336c6c3fb519d" type="text/javascript" integrity="sha256-9/aliU8dGd2tb6OSsuzixeV4y/faTqgFtohetphbbj0=" crossorigin="anonymous"></script>
  <script src="{{ 'webflow.js' | asset_url }}" type="text/javascript"></script>
  <!-- [if lte IE 9]><script src="https://cdnjs.cloudflare.com/ajax/libs/placeholders/3.0.2/placeholders.min.js"></script><![endif] -->

{% include 'notifications' %}{{ 'udesly-shopify.min.js' | asset_url | script_tag }}{{ 'option_selection.js' | shopify_asset_url | script_tag }}