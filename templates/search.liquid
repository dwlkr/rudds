{% layout 'search' %}
      
  <div class="div-block-348">
    <div class="utility-page-wrap">
      <div class="utility-page-content w-form"><img src="{{ '404-error.svg' | asset_url }}" alt="" class="image-59">
        <h2 class="heading-84">{{ 'search.H2_0' | t }}</h2><a href="/" class="sr-button invert w-button">{{ 'search.A_1' | t }}</a></div>
    </div>
  </div>
  <div class="container-5 w-container"></div>
  <div style="display:none" class="main-section">
    <div class="category-hero">
      <div class="div-block-303">
        <h1 class="search-results-label">{{ 'search.H1_2' | t }}</h1>
        {% if search.performed %}<h1 class="search-query">{{ search.terms }}</h1>{% endif %}
      </div>
      <form action="/search" class="search-products w-form" method="get" role="search"><input type="search" class="search-input w-input" maxlength="256" name="q" placeholder="Search another productâ€¦" id="search" required="" value="{{ search.terms | escape }}"><input type="submit" value="Search" class="main-button search-box w-button"></form>
    </div>
    <div class="section-20">
      {% paginate search.results by settings.search_results_limit %}<div class="collection-list-wrapper-2 w-dyn-list">
        {% unless search.results_count == 0 %}<div role="list" class="collection-list-2 w-dyn-items">
          {% for item in search.results %}<div role="listitem" class="w-dyn-item">
            <div class="collection-item__card-product"><a href="{{ item.url }}" class="card-product_wrapper-image w-inline-block"><img src="{{ item | img_url: 'large' }}" alt="{{ item.alt }}" class="wrapper-image__img"></a>
              <div class="card-blog__wrapper-text">
                <div class="wrapper-text__line-2"></div>
                <div class="link__text-2 blog"></div>
                <h1 class="wrapper--text__title">{{ item.title }}</h1>
              </div>
            </div>
          </div>{% endfor %}
        </div>
        {% else %}<div class="w-dyn-empty">
          <div>{{ 'search.DIV_3' | t }}</div>
        </div>{% endunless %}
      </div>{% endpaginate %}
    </div>
  </div>
  <script src="https://d3e54v103j8qbb.cloudfront.net/js/jquery-3.5.1.min.dc5e7f18c8.js?site=5f61d645b88336c6c3fb519d" type="text/javascript" integrity="sha256-9/aliU8dGd2tb6OSsuzixeV4y/faTqgFtohetphbbj0=" crossorigin="anonymous"></script>
  <script src="{{ 'webflow.js' | asset_url }}" type="text/javascript"></script>
  <!-- [if lte IE 9]><script src="https://cdnjs.cloudflare.com/ajax/libs/placeholders/3.0.2/placeholders.min.js"></script><![endif] -->

{% include 'notifications' %}{{ 'udesly-shopify.min.js' | asset_url | script_tag }}{{ 'option_selection.js' | shopify_asset_url | script_tag }}