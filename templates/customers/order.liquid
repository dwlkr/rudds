{% layout 'customers_order' %}
      
  <div class="div-block-348">
    <div class="utility-page-wrap">
      <div class="utility-page-content w-form"><img src="{{ '404-error.svg' | asset_url }}" alt="" class="image-59">
        <h2 class="heading-84">{{ 'customers_order.H2_0' | t }}</h2><a href="/" class="sr-button invert w-button">{{ 'customers_order.A_1' | t }}</a></div>
    </div>
  </div>
  <div data-node-type="commerce-order-confirmation-wrapper" data-wf-order-query="" data-wf-page-link-href-prefix="" class="w-commerce-commerceorderconfirmationcontainer order-confirmation">
    <div class="w-commerce-commercelayoutcontainer w-container">
      <div class="w-commerce-commercelayoutmain">
        <div class="w-commerce-commercecheckoutcustomerinfosummarywrapper">
          <div class="w-commerce-commercecheckoutsummaryblockheader">
            <h4>{{ 'customers_order.H4_2' | t }}</h4>
          </div>
          <fieldset class="w-commerce-commercecheckoutblockcontent">
            <div class="w-commerce-commercecheckoutrow">
              <div class="w-commerce-commercecheckoutcolumn">
                <div class="w-commerce-commercecheckoutsummaryitem"><label class="w-commerce-commercecheckoutsummarylabel">{{ 'customers_order.LABEL_3' | t }}</label>
                  <div>{{ order.shipping_address | format_address }}</div>
                </div>
              </div>
            </div>
          </fieldset>
        </div>
        <div class="w-commerce-commercecheckoutshippingsummarywrapper">
          <div class="w-commerce-commercecheckoutsummaryblockheader">
            <h4>{{ 'customers_order.H4_4' | t }}</h4>
          </div>
          <fieldset class="w-commerce-commercecheckoutblockcontent">
            <div class="w-commerce-commercecheckoutrow">
              <div class="w-commerce-commercecheckoutcolumn">
                {% for shipping in order.shipping_methods %}<div class="w-commerce-commercecheckoutsummaryitem">
                  <div>{{ shipping.title }}</div>
                  <div>{{ shipping.price | money }}</div>
                </div>{% endfor %}
              </div>
            </div>
          </fieldset>
        </div>
        <div class="w-commerce-commercecheckoutpaymentsummarywrapper">
          <div class="w-commerce-commercecheckoutsummaryblockheader">
            <h4>{{ 'customers_order.H4_5' | t }}</h4>
          </div>
          <fieldset class="w-commerce-commercecheckoutblockcontent block-content">
            <div class="w-commerce-commercecheckoutrow">
              <div class="w-commerce-commercecheckoutcolumn">
                <div class="w-commerce-commercecheckoutsummaryitem"><label class="w-commerce-commercecheckoutsummarylabel">{{ 'customers_order.LABEL_6' | t }}</label>
                  <div class="w-commerce-commercecheckoutsummaryflexboxdiv">
                    <div order="payment-method" class="w-commerce-commercecheckoutsummarytextspacingondiv"></div>
                    <div class="w-commerce-commercecheckoutsummarytextspacingondiv"></div>
                  </div>
                  <div class="w-commerce-commercecheckoutsummaryflexboxdiv">
                    <div></div>
                    <div>{{ 'customers_order.DIV_7' | t }}</div>
                    <div></div>
                  </div>
                </div>
              </div>
              <div class="w-commerce-commercecheckoutcolumn">
                <div class="w-commerce-commercecheckoutsummaryitem"><label class="w-commerce-commercecheckoutsummarylabel">{{ 'customers_order.LABEL_8' | t }}</label>
                  <div>{{ order.billing_address | format_address }}</div>
                </div>
              </div>
            </div>
          </fieldset>
        </div>
      </div>
      <div class="w-commerce-commercelayoutsidebar">
        <div class="w-commerce-commercecheckoutordersummarywrapper">
          <div class="w-commerce-commercecheckoutsummaryblockheader">
            <h4>{{ 'customers_order.H4_9' | t }}</h4>
          </div>
          <fieldset class="w-commerce-commercecheckoutblockcontent">
            <div class="w-commerce-commercecheckoutsummarylineitem">
              <div>{{ 'customers_order.DIV_10' | t }}</div>
              <div>{{ order.subtotal_price | money }}</div>
            </div>
            {% for tax_line in order.tax_lines %}<div class="w-commerce-commercecheckoutsummarylineitem total-line-item">
              <div>{{ tax_line.title }} ({{ tax_line.rate | times: 100 }}%)}</div>
              <div>{{ tax_line.price | money }}</div>
            </div>{% endfor %}
            <div class="w-commerce-commercecheckoutsummarylineitem">
              <div>{{ 'customers_order.DIV_11' | t }}</div>
              <div class="w-commerce-commercecheckoutsummarytotal">{{ order.total_price | money }}</div>
            </div>
          </fieldset>
        </div>
      </div>
    </div>
  </div>
  <div class="section-25">
    <div class="div-block-309">
      <div class="w-commerce-commercecheckoutsummaryblockheader">
        <h4>{{ 'customers_order.H4_12' | t }}</h4>
      </div>
      <div class="block-content">
        <div class="collection-list-wrapper-3 w-dyn-list">
          <div role="list" class="collection-list-10 w-dyn-items">
            {% for line_item in order.line_items %}<div role="listitem" class="w-dyn-item">
              <div class="order-items">
                <div class="variations-main-wrapper">
                  <div class="mini-cart-featured-image" style="background-image: url({{ line_item.product.featured_image | img_url: 'large' }})"></div>
                  <div class="minicart-info">
                    <div class="text-block-38">{{ line_item.title }}</div>
                    <div class="text-block-39">{{ line_item.quantity | times: line_item.price | money }}</div>
                  </div>
                </div>
                <div class="cart-quantity-3">{{ line_item.quantity }}</div>
              </div>
            </div>{% endfor %}
          </div>
          <div class="w-dyn-empty">
            <div>{{ 'customers_order.DIV_13' | t }}</div>
          </div>
        </div>
      </div>
    </div>
  </div>
  <script src="https://d3e54v103j8qbb.cloudfront.net/js/jquery-3.5.1.min.dc5e7f18c8.js?site=5f61d645b88336c6c3fb519d" type="text/javascript" integrity="sha256-9/aliU8dGd2tb6OSsuzixeV4y/faTqgFtohetphbbj0=" crossorigin="anonymous"></script>
  <script src="{{ 'webflow.js' | asset_url }}" type="text/javascript"></script>
  <!-- [if lte IE 9]><script src="https://cdnjs.cloudflare.com/ajax/libs/placeholders/3.0.2/placeholders.min.js"></script><![endif] -->

{% include 'notifications' %}{{ 'udesly-shopify.min.js' | asset_url | script_tag }}{{ 'option_selection.js' | shopify_asset_url | script_tag }}